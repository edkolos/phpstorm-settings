  <template name="forr" value="for (let i = $ARRAY$.length - 1; i &gt;= 0; i--) {&#10;  let $VAR$ = $ARRAY$[i];&#10;  $END$&#10;}" description="Iterate elements of array in reverse order" toReformat="true" toShortenFQNames="true">

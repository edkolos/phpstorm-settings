  </template>
  <template name="for" value="for (let i = 0, length = $ARRAY$.length; i &lt; length; i++) {&#10;    let $VAR$ = $ARRAY$[i];&#10;    $END$&#10;}" description="for" toReformat="true" toShortenFQNames="true">

<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="FONT_SCALE" value="1.25" />
    <option name="LINE_SPACING" value="1.2" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="TypeScript" value="true" />
      </map>
    </option>
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
  </component>
  <component name="LiveEditOptions" autoDelay="1" restartIfHotSwapFailed="true" updatePolicy="AUTO" />
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>